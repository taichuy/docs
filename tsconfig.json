{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "@/*": ["./*"]
    }
  },
  "include": ["typings.d.ts", ".dumirc.ts", ".dumi/**/*", "docs/**/*"],
  "exclude": ["node_modules", "dist", ".dumi/tmp"]
}
